/*File:~/custom/content/themes/Base/CSS/home.css*/
#newsletter-modal-home {
	position: fixed;
    top: 20%;
    display: flex;
    background-color: white;
    padding: 35px 20px;
	padding-top: 40px;
    width: 75%;
    left: 0;
    right: 0;
    z-index: 99;
}
#newsletter-modal-home.hide {
	display: none !important;
}
#newsletter-modal-home #btn-close {
	position: absolute;
	right: 10px;
	top: 5px;
	font-size: 20px;
}
#newsletter-modal-home .image .wd-marketing-banner .banner-wrapper img{
	max-width: 600px;
	height: 300px;
	width: 100%;
}
#newsletter-modal-home .form .wd-marketing-newsletter .wd-header {
	display: none;
}
#newsletter-modal-home .form .wd-marketing-newsletter .wd-content p {
	font-size: 30px;
	margin-bottom: 10px;
}
#newsletter-modal-home .form label {
	line-height: 40px;
	font-size: 16px;
	margin: 0;
}
#newsletter-modal-home form button {
	margin-top: 16px;
	float: right;
	border-radius: 0;
}
#newsletter-modal-home p.title {
	font-size: 28px;
}
#newsletter-modal-home .success {
	color: green;
}
#newsletter-modal-home .error {
	color: red;
}
body.newsletter-open:before {
	content: '';
	position: fixed;
	top: 0;
	left: 0;
	background-color: rgba(0,0,0,0.5);
	width: 100%;
	height: 100%;
	z-index: 70;
}
